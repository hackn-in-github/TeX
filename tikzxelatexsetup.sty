 \else
  \expandafter\@Hako@Data@Split\@Hako@Data\@nil
 \fi
 \draw[thick,#2](\@HakoHige@Number-1/2*\@Hako@Width,\@HakoHige@FQua@Num)%
       rectangle(\@HakoHige@Number+1/2*\@Hako@Width,\@HakoHige@TQua@Num)

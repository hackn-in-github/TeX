% #3 ゼロ詰めした数字を収めるコントロールシーケンス
% ex. \ZeroPlus{135}{5}\NO ---> \NO=00135
\def\ZeroPlus#1#2#3{%
 \bgroup
  \edef\@NoBody{#1}%
  \@tempcnta=\z@\relax
  \expandafter\@tfor\expandafter\member\expandafter:\expandafter=\@NoBody\do{%
   \advance\@tempcnta by\@ne\relax
  }%
  \edef\@NoBody{}%
  \loop
   \ifnum\@tempcnta<#2\relax
   \advance\@tempcnta by\@ne\relax
   \edef\@NoBody{\@NoBody0}%
  \repeat
  \xdef#3{\@NoBody#1}%
 \egroup
}%

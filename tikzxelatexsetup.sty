     ($(\x{A},\y{A})!\@MTAnum/\@MTAMAX!(\x{B},\y{B})!.5\@tempdimb!-90:(\x{O},\y{O})$);
  }%
 \else
  \ifnum\@MTAnumber>\z@\relax
   \draw[MTAstyle] let \p1=($(\@MTAresult:#4)+#3$) in
    ($(\x1,\y1)!.5\@tempdimb!#3$)--($(\x1,\y1)!-.5\@tempdimb!#3$);
  \fi
 \fi
 \egroup
 \mytikzoutlinefalse
}%

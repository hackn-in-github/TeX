 \edef\@DS@Sub@rightratio{\@DS@Sub@overratio}%
 \setkeys{DrawSegmentsSub}{#2}%
 \draw[#1,#3]($(@DS@F@Point)!-(\@DS@Sub@leftratio)!(@DS@E@Point)$)--($(@DS@E@Point)!-(\@DS@Sub@rightratio)!(@DS@F@Point)$);%

\def\@GetAngle#1,#2,#3@#4{%
 \tikzmath{%
  real \@x;
  coordinate \@c;
  \@c1=($(#1)-(#2)$);
  \@c2=($(#3)-(#2)$);
  \@x=atan2(\@cy2,\@cx2)-atan2(\@cy1,\@cx1);
  if \@x>360 then {\@x=\@x-360;};%
  if \@x<0 then {\@x=360+\@x;};%
 }%
 \let#4\@x%
}%
%% \MathAtan{#1}#2
%% 原点と点(#1)とのなす角を0から360の範囲で計算したものを#2に格納する
%% 小数第1位を四捨五入している
\def\MathAtan#1#2{%
 \tikzmath{%
  real \@x;
  coordinate \@c;
  \@c=(#1);
  \@x=atan2(\@cy,\@cx);
  if \@x<0 then {\@x=360+\@x;};
 }%
 \let#2\@x
}%

%% \GetSubArray{#1}{#2}{#3}{#4}%
%%% #1 : 切り出す列の元となる列
%%% #2 : 切り出す列の元となる列の最初の元の番号
%%% #3 : 切り出す列の元となる列の最後の元の番号
%%% #4 : 切り出す列の名前
\NewDocumentCommand\GetSubArray{mmmm}{%
 \def#4{\@gobble}%
 \foreach \N in {1,...,#3}{%
  \pgfmatharray{#1}{\N-1}%
  \ifthenelse{\N=#2\OR\N>#2}{\xdef#4{#4,\pgfmathresult}}{}%
 }%
}%
%% \GetArrayMed{#1}{#2}%
%%% #1 : 中央値を調べる列
%%% #2 : 中央値を収めるコントロールシーケンス
\NewDocumentCommand\GetArrayMed{mm}{%
 \c@tempcnta=\z@
 \expandafter\@for\expandafter\member\expandafter:\expandafter=#1\do{%
  \advance\c@tempcnta by\@ne
 }%
 \ifodd\c@tempcnta
  \pgfmatharray{#1}{(\c@tempcnta-1)/2}%
  \edef#2{\pgfmathresult}%
 \else
  \pgfmatharray{#1}{\c@tempcnta/2-1}%
  \let\@temp@getarray@f=\pgfmathresult
  \pgfmatharray{#1}{\c@tempcnta/2}%
  \let\@temp@getarray@s=\pgfmathresult
  \tikzmath{\@temp@med=(\@temp@getarray@f+\@temp@getarray@s)/2;}
  \edef#2{\@temp@med}%
 \fi
}%

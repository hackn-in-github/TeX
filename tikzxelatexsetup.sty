 \bgroup
  \let\@xp=\expandafter
%  \mytikzcoordinateset{@MTGA}{#1}%
%  \@xp\coordinate\@xp(\@xp @\@xp M\@xp T\@xp G\@xp A\@xp A\@xp)\@xp a\@xp t\@MTGApre;

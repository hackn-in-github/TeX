  \tikz[x=.4em,y=.4em,scale=1]{%
   \def\X{.8}%
   \def\Vr{.6}%
   \coordinate(O1)at(-\X,0);
   \coordinate(O2)at(\X,0);
   \path[name path=R1](O1)circle[radius=\Vr];%
   \path[name path=R2](O2)circle[radius=\Vr];%
   \GetMathparse{sqrt(pow(\X,2)-pow(\Vr,2))}\R

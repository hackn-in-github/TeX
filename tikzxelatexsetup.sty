%%     min amari余りが:ここで指定した値以下になるまで割り算を行う(default:1)
%%  #2と#3の最大公約数を調べる
\tikzset{EuclidDiv/.style={outer~xsep=3pt,y={(0,1.2em)}}}
\keys_define:nn {EuclidDiv}{%
 min amari .code:n = {\int_set:Nn \l_tmp_min_amari_int {#1}},
 min amari .default:n = \c_one
}%
\NewDocumentCommand \EuclidDiv{omm}{%
 \group_begin:
  \int_set:Nn \l_tmp_min_amari_int \c_one
  \IfValueT{#1}{%
   \keys_set:nn {EuclidDiv}{#1}
  }
  \int_zero:N \l_tmp_node_number_int
  % node や coordinate は global であるからグルーピングの影響は受けない
  \int_compare:nNnTF {#2} > {#3}
   {%
    \node(余り0)[inner~sep=0pt]{#3};
    \Original_EuclidDiv:nn {#2}{#3}
   }{
    \node(余り0)[inner~sep=0pt]{#2};
    \Original_EuclidDiv:nn {#3}{#2}
   }
